import java.util.Scanner;

public class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the Number: ");
        int num = sc.nextInt();
        System.out.println("The number from 1 to "+ num + " are: ");
        for(int i = 1; i <=num ;i++){
            System.out.print(i+" ");
        }
    }
}
