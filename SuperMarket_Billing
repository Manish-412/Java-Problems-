import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	  Scanner sc = new Scanner(System.in);
		System.out.print("Enter number of items: ");
		int n = sc.nextInt();
		double total = 0;
		System.out.println("Enter item prices:");
		for (int i = 0; i < n; i++) {
			double price = sc.nextDouble();
			double finalPrice;
			if (price >= 500) {
				finalPrice = price * 0.80; // 20% discount
			} else if (price >= 200) {
				finalPrice = price * 0.90; // 10% discount
			} else {
				finalPrice = price; // no discount
			}
			total += finalPrice;
		}

		// Print rounded total as integer rupees
		System.out.println("Total Bill after discount = " + (long)Math.round(total));
		sc.close();
	}
}
